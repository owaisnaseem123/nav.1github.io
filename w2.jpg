<script type="text/javascript">

    /*Initialization*/
    var allDocs = [];
    var allSpec = [];
    var specialityUID = null;
    var providerUID = null;



    /***Methods***/

    //Reset Click Event All Parameters
    function ClearAll() {
        providerUID = null;
        specialityUID = null;
    };

    //Fill Doctors Auto Complete Suggestion List
    function FillDoctors() {
        var parentElem = $("#doctorOptions").children();
        if (parentElem) {
            var arr = [];
            for (var i = 0; i < parentElem.length; i++) {
                var guid = $(parentElem[i]).text();
                var name = $(parentElem[i]).attr('value');
                arr.push({ value: name, label: name, id: guid });
            }
            allDoctors = arr;
        }
        //Auto Complete Event
        $("#find-a-doctor2").autocomplete({
            source: arr,
            select: function (event, ui) {
                providerUID = ui.item.id;
            }
        }).data("ui-autocomplete")._renderItem = function (ul, item) {
            let txt = String(item.value).replace(new RegExp(this.term, "gi"), "<span class='highlight'>$&</span>");
            return $("<li></li>")
                .data("ui-autocomplete-item", item)
                .append("<a>" + txt + "</a>")
                .appendTo(ul);
        };

    };





    //Run On Form Submit
    function SearchDoctor2() {
       
        debugger
        var ProviderUID = providerUID;
        var spe = specialityUID;
        var doct = $("#find-a-doctor2").val();
        var url = "/Doctors";
        var firstItem = true;

        if (ProviderUID != null) {
            doct = null;
        }

        if (spe) {
            if (firstItem)
                url += "?spe=" + spe;
            else
                url += "&spe=" + spe;
            firstItem = false;
        }

        if (ProviderUID) {
            if (firstItem)
                url += "?providerUId=" + ProviderUID;
            else
                url += "&providerUId=" + ProviderUID;
            firstItem = false;
        }

        if (doct) {
            if (firstItem)
                url += "?doc=" + doct;
            else
                url += "&doc=" + doct;
            firstItem = false;
        }
        window.location = url;
    };

    function SearchLocation() {
        var loc = $("#locationDropdown").val()
        if (loc != null) {
            $("#LocationName").val($("#locationDropdown").val());
        }
    }
    //Fill Department Auto Complete Suggestion List
    function FillSpecialities2(data) {
        if (data) {
            var arr = [];
            for (var i = 0; i < data.length; i++) {
                var guid = data[i].uid;
                var name = data[i].titleName;
                arr.push({ value: name, label: name, id: guid });
            }
            allSpecialities = arr;
        }
        $("#find-a-doctor-by-dept").autocomplete({
            source: arr,
            select: function (event, ui) {
                specialityUID = ui.item.id;
            }
        });
    };

    function FindDoc() {
      
        var doct = $("#find-a-doctor2").val();
        var url = "/Doctors";
        if (doct) {
            url += "?doc=" + doct;
        }
        window.location = url;
    };


    
    //Windows Load Function
    $(window).load(function () {

        //Fill Doctors List Suggestion on Auto Complete
        FillDoctors();
    });

    //Get Server Call
    var url2 = "/Speciality/GetAll";
    $.get(url2, function (data) {
        allSpec = data.data;
        FillSpecialities2(data.data);
    });

    //Keyup Event of Select Doctor Textbox
    $("#find-a-doctor2").on("keyup", function (e) {
        if (e.which == '13') {
            event.preventDefault();
            SearchDoctor2();
        }
    });

    //Validate Form Function
    $("#contact_form").validate({
        submitHandler: function (form) {
            var form_btn = $(form).find('button[type="submit"]');
            var form_result_div = '#form-result';
            $(form_result_div).remove();
            //form_btn.before('<div id="form-result" class="alert alert-success" role="alert" style="display: none;">Request Sent</div>');
            var form_btn_old_msg = form_btn.html();
            form_btn.html(form_btn.prop('disabled', true).data("loading-text"));
            $(form).ajaxSubmit({
                dataType: 'json',
                success: function (data) {
                    if (data == true) {
                        $(form).find('.form-control').val('Done');
                        $("#contact_form").find("input,select,textarea").val("");
                        $("#contact_form").find("select").val("0");
                        form_btn.before('<div id="form-result" class="alert alert-success" role="alert" style="display: none;">Request Sent</div>');
                    } else {
                        form_btn.before('<div id="form-result" class="alert alert-danger" role="alert" style="display: none;">Error</div>');
                    }
                    form_btn.prop('disabled', false).html(form_btn_old_msg);
                    $(form_result_div).html(data.message).fadeIn('slow');
                    setTimeout(function () {
                        $(form_result_div).fadeOut('slow')
                    }, 6000);
                }
            });
        }
    });


</script>


